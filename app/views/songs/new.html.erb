<h1>New Song Form</h1>
<%= form_for(@song) do |f| %>
    <label for="Song Name">Song Name:</label> <br>
    <%= f.text_field :name %> <br> <br>

    <label for="Artist Id">Artist Id:</label> <br>
    <%= f.text_field :artist_id %> <br> <br>
    
    <label for="Genre Id">Genre Id:</label> <br>
    <%= f.text_field :genre_id %> <br> <br>
   
    <%= f.submit %>
<% end %>